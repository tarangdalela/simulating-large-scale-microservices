            "type": "Normal",
            "parameters": {
              "mean": 80,
              "stdev": 10
            }
          },
          "error_rate": {
            "type": "percentage",
            "value": 0.05
          }
        }
      }
    }
  },
  "load": {
    "entry_points": [
      {
        "service": "ServiceA",
        "method": "ProcessOrder",
        "requests_per_second": 10
      }
    ]
  }
