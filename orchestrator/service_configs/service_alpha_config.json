{
  "container_port": 50051,
  "methods": {
    "initiate_workflow": {
      "calls": [
        [
          "service_beta.validate_request",
          "service_gamma.lookup_resources"
        ]
      ],
      "latency_distribution": {
        "type": "normal",
        "parameters": {
          "mean": 50.0,
          "stdev": 10.0
        }
      },
      "error_rate": {
        "type": "bernoulli",
        "parameters": {
          "p": 0.01
        }
      }
    },
    "health_check": {
      "calls": [],
      "latency_distribution": {
        "type": "normal",
        "parameters": {
          "mean": 10.0,
          "stdev": 2.0
        }
      },
      "error_rate": {
        "type": "bernoulli",
        "parameters": {
          "p": 0.0
        }
      }
    },
    "process_step_2": {
      "calls": [
        [
          "service_epsilon.generate_report_data"
        ],
        [
          "service_gamma.archive_intermediate_result"
        ]
      ],
      "latency_distribution": {
        "type": "normal",
        "parameters": {
          "mean": 60.0,
          "stdev": 12.0
        }
      },
      "error_rate": {
        "type": "bernoulli",
        "parameters": {
          "p": 0.03
        }
      }
    },
    "process_step_1": {
      "calls": [
        [
          "service_delta.perform_action_x"
        ]
      ],
      "latency_distribution": {
        "type": "normal",
        "parameters": {
          "stdev": 15.0,
          "mean": 75.0
        }
      },
      "error_rate": {
        "type": "bernoulli",
        "parameters": {
          "p": 0.05
        }
      }
    },
    "aggregate_results": {
      "calls": [
        [
          "service_beta.fetch_summary"
        ],
        [
          "service_delta.fetch_details"
        ],
        [
          "service_epsilon.finalize_status"
        ]
      ],
      "latency_distribution": {
        "type": "normal",
        "parameters": {
          "mean": 120.0,
          "stdev": 30.0
        }
      },
      "error_rate": {
        "type": "bernoulli",
        "parameters": {
          "p": 0.02
        }
      }
    }
  }
}